
@model List<ProductListComponentViewModel>


<table class="table table-dark">

    <thead>
        <tr>
            <th>Id </th>     
            <th>Açıklama </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var items in Model)
        {
            <tr>      
                <td>@items.Name </td>
                <td>@items.Description </td>
            </tr>
        }
    </tbody>

</table>
