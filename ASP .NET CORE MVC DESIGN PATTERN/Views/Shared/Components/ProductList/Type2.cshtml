@model List<ProductListComponentViewModel>

    <a class="btn btn-outline-danger" asp-controller="products" asp-action="Index">ÜRÜNLERE GİT</a>

<table class="table table-dark">

    <thead>
        <tr>
            <th>Id </th>
            <th>Açıklama </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var items in Model)
        {
            <tr>
                <td>@items.Name </td>
                <td>@items.Description </td>
            </tr>
        }
    </tbody>

</table>
