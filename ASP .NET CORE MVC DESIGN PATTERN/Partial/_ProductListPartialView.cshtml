@using EFCORE_VERİALMA.Models.ViewModels;



@model ProductListPartialViewModel


<table class="table table-dark">

    <thead>
        <tr>
            <th>Id </th>
            <th>İsim </th>
            <th> Fiyat</th>
            <th>Stok </th>
        </tr>
    </thead>
    <tbody>

        @foreach(var items in Model.Products)
        {
            <tr>
                <td>@items.Id </td>
                <td>@items.Name </td>
                <td>@items.Price</td>
                <td>@items.Stock </td>
            </tr>
        }
    </tbody>

</table>






